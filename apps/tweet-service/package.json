  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migration:generate": "npm run build && npx typeorm-ts-node-commonjs migration:generate -d data-source.ts",
    "migration:run": "npm run build && npx typeorm-ts-node-commonjs migration:run -d data-source.ts",
    "migration:revert": "npm run build && npx typeorm-ts-node-commonjs migration:revert -d data-source.ts",
    "migration:show": "npm run build && npx typeorm-ts-node-commonjs migration:show -d data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.0",
